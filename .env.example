# Environment Variables Template for SchoolFee.NG
# Copy this file to .env and fill in your actual values

# Database Configuration (Optional - app works without these)
VITE_SUPABASE_URL=your_supabase_project_url_here
VITE_SUPABASE_ANON_KEY=your_supabase_anon_key_here

# AI Services (Optional - app provides mock responses without these)
VITE_GEMINI_API_KEY=your_google_gemini_api_key_here
VITE_GOOGLE_AI_API_KEY=your_google_ai_api_key_here

# App Configuration
VITE_APP_NAME=SchoolFee.NG
VITE_APP_ENV=production

# Contact Information
VITE_SUPPORT_EMAIL=support@schoolfee.ng
VITE_SUPPORT_PHONE=+234-809-876-5432

# Feature Flags (Optional)
VITE_ENABLE_AI_FEATURES=true
VITE_ENABLE_OFFLINE_MODE=true
VITE_ENABLE_ANALYTICS=false